{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/angular@2",
      "config": { "outputDirectory": "dist/sala-de-juegos/browser" }
    }
  ],
  "routes": [
    {
      "src": "/(.*\\.(?:js|css|map|json|png|jpg|jpeg|gif|svg|ico|woff2|woff|ttf|eot))$",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable",
        "Content-Type": "application/javascript"
      },
      "continue": true
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}