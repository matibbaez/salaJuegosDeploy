{
  "version": 2,
  "routes": [
    {
      "src": "/(.*\\.(?:js|css|map|json|png|jpg|jpeg|gif|svg|ico|woff2|woff|ttf|eot))$",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable",
        "Content-Type": "application/javascript"
      },
      "continue": true
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}